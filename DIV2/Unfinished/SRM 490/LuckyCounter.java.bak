import java.util.*;

public class LuckyCounter {
      public int countLuckyMoments(String[] moments) {
		int count = 0;
		for(String moment:moments) {
			boolean spec = false;
			char a = moment.charAt(0);
			char b = moment.charAt(1);
			char c = moment.charAt(3);
			char d = moment.charAt(4);
			if(a == b && c == d)
			spec=true;
			if(a==c && b==d)
			spec=true;
			if(a==d && b==c)
			spec=true;
			if(spec)
			count++;
		}
		return count;
	}
       
